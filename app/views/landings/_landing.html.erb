<div id="<%= dom_id landing %>" class="p-2 my-2 border border-slate-200 bg-slate-300">
  <div class="flex justify-center items-center">
    <p class="border border-gray-400 w-24 pl-1 bg-white">
      <%= landing.first_name %>
    </p>
    <p class="border border-gray-400 w-24 pl-1 bg-white">
      <%= landing.last_name %>
    </p>
    <p class="border border-gray-400 w-56 pl-1 bg-white">
      <%= landing.email %>
    </p>
    <p class="border border-gray-400 w-40 pl-1 bg-white">
      <%= landing.phone %>
    </p>
    <p class="border border-gray-400 w-56 pl-1 bg-white">
      <%= landing.location %>
    </p>
    <p class="border border-gray-400 w-24 pl-1 bg-white">
      <%= landing.created_at.to_date.strftime("%m/%d/%y") %>
    </p>
  </div>
  <div class="w-full justify-center items-center">
    <%= form_for(landing, class: "flex w-full") do |f| %>
      <div class="flex flex-col mx-3">
        <%= f.label :content, "Notes"%>
        <%= f.text_field :content, class: "w-full" %>
      </div>
      <%= f.submit "Update", class: "text-white bg-blue-700 hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 font-medium rounded-lg text-sm w-full sm:w-auto mt-4 px-5 py-2.5 text-center dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800 cursor-pointer" %>
    <% end %>
  </div>
</div>
